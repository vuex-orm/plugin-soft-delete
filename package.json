{
  "name": "@vuex-orm/plugin-soft-delete",
  "version": "1.2.0",
  "description": "Vuex ORM plugin for adding soft delete capability.",
  "main": "dist/vuex-orm-soft-delete.common.js",
  "module": "dist/vuex-orm-soft-delete.esm.js",
  "unpkg": "dist/vuex-orm-soft-delete.js",
  "typings": "lib/index.d.ts",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "build": "yarn clean && yarn build:ts && yarn build:main",
    "build:ts": "tsc -p tsconfig.build.json",
    "build:main": "node build/build.js",
    "lint": "prettier --write --parser typescript \"{src,test}/**/*.ts?(x)\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --collect-coverage",
    "clean": "rm -rf dist && rm -rf lib && rm -rf coverage",
    "docs": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:deploy": "bash ./docs/deploy.sh"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuex-orm/plugin-soft-delete.git"
  },
  "keywords": [
    "orm",
    "vuex-orm",
    "vuex-plugin",
    "vuex-orm-plugin",
    "soft-delete",
    "change-tracking"
  ],
  "author": "Kia Ishii",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vuex-orm/plugin-soft-delete/issues"
  },
  "peerDependencies": {
    "@vuex-orm/core": ">=0.34.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@types/jest": "^25.1.4",
    "@vuex-orm/core": "^0.36.3",
    "codecov": "^3.6.5",
    "jest": "^25.2.4",
    "lint-staged": "^10.0.10",
    "prettier": "1.19.1",
    "rollup": "^2.3.0",
    "ts-jest": "^25.3.0",
    "typescript": "^3.8.3",
    "uglify-js": "^3.8.1",
    "vue": "^2.6.11",
    "vue-server-renderer": "^2.6.11",
    "vuepress": "^1.4.0",
    "vuepress-theme-vue": "^1.1.0",
    "vuex": "^3.1.3"
  }
}
